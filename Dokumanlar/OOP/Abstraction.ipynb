    "from abc import ABC,abstractclassmethod,abstractmethod\n",
    "\n",
    "class Besin(ABC):\n",
    "    @abstractmethod\n",
    "    def yika(self):\n",
    "        pass\n",
    "\n",
    "class Cilek(Besin):\n",
    "    def yika(self):\n",
    "        print(\"Çilekler yıkandı\")\n",
    "\n",
    "obj = Cilek()"
